@prefix ndt: <http://example.org/ndt-ontology#> .
@prefix ex: <http://example.org/ndt-instance#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# Classes (TBox)
ndt:Infrastructure a owl:Class .
ndt:Bridge rdfs:subClassOf ndt:Infrastructure .
ndt:Material a owl:Class .
ndt:Defect a owl:Class .
ndt:DeteriorationMechanism a owl:Class .
ndt:NDTMethod a owl:Class .
ndt:InspectionPlan a owl:Class .
ndt:InspectionTask a owl:Class .

# Object Properties (TBox)
ndt:hasMaterial a owl:ObjectProperty ;
    rdfs:domain ndt:Infrastructure ;
    rdfs:range  ndt:Material .
ndt:hasDefect a owl:ObjectProperty ;
    rdfs:domain ndt:Infrastructure ;
    rdfs:range  ndt:Defect .
ndt:causes a owl:ObjectProperty ;
    rdfs:domain ndt:DeteriorationMechanism ;
    rdfs:range  ndt:Defect .
ndt:detects a owl:ObjectProperty ;
    rdfs:domain ndt:NDTMethod ;
    rdfs:range  ndt:Defect .
ndt:isDetectedBy a owl:ObjectProperty ;  # inverse of detects
    owl:inverseOf ndt:detects .
ndt:recommendsMethod a owl:ObjectProperty ;
    rdfs:domain ndt:InspectionPlan ;
    rdfs:range ndt:NDTMethod .
ndt:addressesDefect a owl:ObjectProperty ;
    rdfs:domain ndt:InspectionPlan ;
    rdfs:range ndt:Defect .
ndt:justifiedBy a owl:ObjectProperty ;
    rdfs:domain ndt:InspectionPlan ;
    rdfs:range rdfs:Resource .
ndt:hasTask a owl:ObjectProperty ;
    rdfs:domain ndt:InspectionPlan ;
    rdfs:range ndt:InspectionTask .

# Example individuals (ABox)
ex:Bridge-1 a ndt:Bridge ;
    ndt:hasMaterial ex:Concrete .
ex:Concrete a ndt:Material .
ex:Cracking a ndt:Defect ;
    ndt:isDetectedBy ex:UltrasonicTesting ;
    ndt:causedBy ex:Corrosion .
ex:Corrosion a ndt:DeteriorationMechanism .
ex:UltrasonicTesting a ndt:NDTMethod ;
    ndt:detects ex:Cracking .
ex:Plan001 a ndt:InspectionPlan ;
    ndt:addressesDefect ex:Cracking ;
    ndt:recommendsMethod ex:UltrasonicTesting ;
    ndt:justifiedBy ex:Cracking .
